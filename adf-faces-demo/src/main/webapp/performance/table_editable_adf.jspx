<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="1.2"
          xmlns:f="http://java.sun.com/jsf/core"
          xmlns:h="http://java.sun.com/jsf/html"
          xmlns:afh="http://myfaces.apache.org/adf/faces/html" 
          xmlns:af="http://myfaces.apache.org/adf/faces" >
  <jsp:directive.page contentType="text/html;charset=utf-8"/>
  <f:view>
 <af:document title="Table Editable ADF">
        <af:form >
            <af:messages/>
            <af:table value="#{periodicTable.tableData}" var="row" rows="0">
              <af:column>
                <f:facet name="header">
                  <af:outputText value="Name"/>
                </f:facet>
                <af:inputText value="#{row.name}"/>
              </af:column>
              <af:column>
                <f:facet name="header">
                  <af:outputText value="Symbol"/>
                </f:facet>
                <af:inputText value="#{row.symbol}"/>
              </af:column>
              <af:column>
                <f:facet name="header">
                  <af:outputText value="Action Column"/>
                </f:facet>
                <af:commandButton text="Action" action="#{row.action}"/>
              </af:column>
            </af:table>
        </af:form>
 </af:document>
  </f:view>
</jsp:root>
