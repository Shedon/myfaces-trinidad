<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="1.2"
          xmlns:f="http://java.sun.com/jsf/core"
          xmlns:h="http://java.sun.com/jsf/html"          
          xmlns:afh="http://myfaces.apache.org/adf/faces/html" 
          xmlns:af="http://myfaces.apache.org/adf/faces" >
  <jsp:directive.page contentType="text/html;charset=iso-8859-1"/>
  <f:view>
    <afh:html>
      <afh:head title="Test of ADF Faces EL bindings"/>
      <afh:body>
       <af:panelPage>
       <af:form>
         <af:messages/>
         <af:panelGroup layout="vertical">
           <af:outputText value="Current context is #{adfFacesContext}"/>
           <af:outputText value="Right to left? :  #{adfFacesContext.rightToLeft}"/>
           <af:outputText value="Look and feel? :  #{adfFacesContext.lookAndFeel}"/>
           <af:outputText value="Accessibility mode? :  #{adfFacesContext.accessibilityMode}"/>
           <af:outputText value="Time zone? :  #{adfFacesContext.timeZone}"/>
           
           <af:outputText value="#{adfFacesContext.formatter['Test {0} MessageFormat']['of']}"/>
           <af:goLink destination="#{adfFacesContext.helpSystem.frontPage}" text="Get Help"/>
           <af:goLink destination="#{adfFacesContext.helpTopic.ui_table}" text="Get Help"/>
         </af:panelGroup>
       </af:form>
       </af:panelPage>
      </afh:body>
    </afh:html>
  </f:view>
</jsp:root>
