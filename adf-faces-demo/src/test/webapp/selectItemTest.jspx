<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="1.2"
          xmlns:f="http://java.sun.com/jsf/core"
          xmlns:h="http://java.sun.com/jsf/html"
          xmlns:afh="http://myfaces.apache.org/adf/faces/html" 
          xmlns:af="http://myfaces.apache.org/adf/faces" >
  <jsp:directive.page contentType="text/html;charset=iso-8859-1"/>
  <f:view>
    <afh:html>
      <afh:head title="Test of selectItemSupport"/>
      <afh:body>
       <af:form>
         <af:panelForm rows="5" maxColumns="3">
           <af:selectOneChoice label="Explicit selectItem">
             <f:selectItem itemValue="foo" itemLabel="Foo"/>
             <f:selectItem itemValue="bar" itemLabel="Bar"/>
             <f:selectItem itemValue="baz" itemLabel="Baz"/>
           </af:selectOneChoice>

           <af:selectOneChoice label="Bound selectItem">
             <f:selectItem value="#{selectItemTest.oneItem}"/>
             <f:selectItem value="#{selectItemTest.itemList[1]}"/>
             <f:selectItem value="#{selectItemTest.itemList[2]}"/>
           </af:selectOneChoice>

           <af:selectOneChoice label="selectItems at list">
             <f:selectItems value="#{selectItemTest.itemList}"/>
           </af:selectOneChoice>

           <af:selectOneChoice label="selectItems at array">
             <f:selectItems value="#{selectItemTest.itemArray}"/>
           </af:selectOneChoice>

           <af:selectOneChoice label="selectItems at map">
             <f:selectItems value="#{selectItemTest.itemMap}"/>
           </af:selectOneChoice>

           <af:selectOneListbox label="Explicit selectItem">
             <f:selectItem itemValue="foo" itemLabel="Foo"/>
             <f:selectItem itemValue="bar" itemLabel="Bar"/>
             <f:selectItem itemValue="baz" itemLabel="Baz"/>
           </af:selectOneListbox>

           <af:selectOneListbox label="Bound selectItem">
             <f:selectItem value="#{selectItemTest.oneItem}"/>
             <f:selectItem value="#{selectItemTest.itemList[1]}"/>
             <f:selectItem value="#{selectItemTest.itemList[2]}"/>
           </af:selectOneListbox>

           <af:selectOneListbox label="selectItems at list">
             <f:selectItems value="#{selectItemTest.itemList}"/>
           </af:selectOneListbox>

           <af:selectOneListbox label="selectItems at array">
             <f:selectItems value="#{selectItemTest.itemArray}"/>
           </af:selectOneListbox>

           <af:selectOneListbox label="selectItems at map">
             <f:selectItems value="#{selectItemTest.itemMap}"/>
           </af:selectOneListbox>

           <af:selectOneRadio label="Explicit selectItem">
             <f:selectItem itemValue="foo" itemLabel="Foo"/>
             <f:selectItem itemValue="bar" itemLabel="Bar"/>
             <f:selectItem itemValue="baz" itemLabel="Baz"/>
           </af:selectOneRadio>

           <af:selectOneRadio label="Bound selectItem">
             <f:selectItem value="#{selectItemTest.oneItem}"/>
             <f:selectItem value="#{selectItemTest.itemList[1]}"/>
             <f:selectItem value="#{selectItemTest.itemList[2]}"/>
           </af:selectOneRadio>

           <af:selectOneRadio label="selectItems at list">
             <f:selectItems value="#{selectItemTest.itemList}"/>
           </af:selectOneRadio>

           <af:selectOneRadio label="selectItems at array">
             <f:selectItems value="#{selectItemTest.itemArray}"/>
           </af:selectOneRadio>

           <af:selectOneRadio label="selectItems at map">
             <f:selectItems value="#{selectItemTest.itemMap}"/>
           </af:selectOneRadio>

         </af:panelForm>
    
         <af:selectOrderShuttle>
           <f:selectItems value="#{selectItemTest.itemList}"/>
         </af:selectOrderShuttle>

         <af:commandButton text="Submit"/>
       </af:form>
      </afh:body>
    </afh:html>
  </f:view>
</jsp:root>
