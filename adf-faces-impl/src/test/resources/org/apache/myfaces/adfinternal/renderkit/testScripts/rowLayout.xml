<test xmlns="http://myfaces.apache.org/adf/view/faces/test"
      xmlns:af="http://myfaces.apache.org/adf/view/faces">
      
  <base-component af:type="org.apache.myfaces.adf.HtmlRowLayout">
    
    <component af:type="org.apache.myfaces.adf.HtmlCellFormat" halign="center">
      <component af:type="org.apache.myfaces.adf.CoreOutputText" value="Row 1"/>
    </component>
    
    <component af:type="org.apache.myfaces.adf.CoreOutputText" value="Row 2"/>
    
  </base-component>
  
  <!-- ungrouped attributes -->
  <enum-test name="halign"/>
  <enum-test name="valign"/>
  <attribute-test name="width" value="75%"/>
  
  <!-- core attributes -->
  <attribute-test name="id" value="myRowId"/>
  <attribute-test name="inlineStyle" value="background-color:blue"/>
  <attribute-test name="shortDesc" value="myShortDesc"/>
  <attribute-test name="styleClass" value="myStyle"/>
  
  <!-- javascript attributes -->
  <js-test/>
  
</test>
