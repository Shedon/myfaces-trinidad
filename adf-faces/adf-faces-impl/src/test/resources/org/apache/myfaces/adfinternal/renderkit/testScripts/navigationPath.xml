<test xmlns="http://myfaces.apache.org/adf/view/faces/test"
       xmlns:af="http://myfaces.apache.org/adf/view/faces">
  <base-component af:type="org.apache.myfaces.adf.CoreNavigationPath"
                  value="#{navigationpath}" var="foo">
    <component af:type="org.apache.myfaces.adf.CoreCommandNavigationItem" 
               af:facet="nodeStamp" 
               text="#{foo.label}"
               id="stamp"/> 
  </base-component>
  
  <!-- ungrouped -->
  <enum-test name="orientation"/>
  <!--attribute-test name="partialTriggers" value="x y" matchesBase="true"/-->
  

  <!-- core -->  
  <attribute-test name="id" value="myid"/>
  <!--attribute-test name="inlineStyle" value="background-color:red"/-->
  <!--boolean-test name="rendered"/-->
  <attribute-test name="shortDesc" value="my short desc"/>
  <!--attribute-test name="styleClass" value="mystyle"/-->
  
  <!-- javascript -->
  <js-test/>  
</test>
