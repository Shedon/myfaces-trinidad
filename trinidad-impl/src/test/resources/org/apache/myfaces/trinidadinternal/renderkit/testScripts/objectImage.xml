<test xmlns="http://myfaces.apache.org/adf/view/faces/test"
       xmlns:af="http://myfaces.apache.org/adf/view/faces">
  <base-component af:type="org.apache.myfaces.trinidad.CoreObjectImage" source="/bogus/image/uri"/>  
  <attribute-test name="imageMapType" value="server"/>
  <attribute-test name="imageMapType" value="none" matchesBase="true"/>
  <attribute-test name="longDescURL" value="/bogus/longdesc/uri"/>
  
   <!-- Core attribute tests -->
  <attribute-test name="id"/>
  <attribute-test name="inlineStyle"/>
  <boolean-test name="rendered"/>
  <attribute-test name="shortDesc"/>
  <attribute-test name="styleClass"/>
  
  <js-test/>
</test>
