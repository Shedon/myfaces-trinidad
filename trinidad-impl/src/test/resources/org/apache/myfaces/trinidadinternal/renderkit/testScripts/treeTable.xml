<?xml version="1.0" encoding="windows-1252" ?>
<test xmlns="http://myfaces.apache.org/adf/view/faces/test"
       xmlns:af="http://myfaces.apache.org/adf/view/faces">
  <base-component af:type="org.apache.myfaces.trinidad.CoreTreeTable" value="#{treeModel}" var="row" rowsByDepth="5" summary="treeTable test">

    <component af:facet="nodeStamp" af:type="org.apache.myfaces.trinidad.CoreColumn">
       <component af:facet="header" af:type="org.apache.myfaces.trinidad.CoreOutputText" value="IntHeader"/>
       <component af:type="org.apache.myfaces.trinidad.CoreOutputFormatted" value="#{row.name}"/>
    </component>

    <component af:type="org.apache.myfaces.trinidad.CoreColumn">
       <component af:facet="header" af:type="org.apache.myfaces.trinidad.CoreOutputText" value="StringHeader"/>
       <component af:type="org.apache.myfaces.trinidad.CoreOutputText" value="#{row.name}"/>
    </component>

    <component af:facet="header" af:type="org.apache.myfaces.trinidad.CoreOutputText" value="headerFacet"/>
    <component af:facet="footer" af:type="org.apache.myfaces.trinidad.CoreOutputText" value="footerFacet"/>

    <component af:facet="pathStamp" af:type="org.apache.myfaces.trinidad.CoreOutputFormatted" value="#{row.name}"/>

  </base-component>

  <!-- NO MATCH FIX -->
  <!-- Result of AttributeTest[banding,none] were not identical to base, but should have been! expected:<...e" -->
  <!--enum-test name="banding" default="none"/-->
  <attribute-test name="rowBandingInterval" value="1"/>
  
  <attribute-test name="columnBandingInterval" value="0" matchesBase="true">
    <attribute-test name="rowBandingInterval" value="0"/>
  </attribute-test>

  <attribute-test name="columnBandingInterval" value="1"/>

  <attribute-test name="id"/>

  <attribute-test name="shortDesc" value="myDetail"/>
  <attribute-test name="styleClass" value="myStyleClass"/>


  <attribute-test name="columnBandingInterval" value="2"/>

  <attribute-test name="disclosedRowKeys" value="#{pathSet}"/>

  <attribute-test name="disclosedRowKeys" value="#{pathSet}">
    <attribute-test name="rowBandingInterval" value="1"/>
  </attribute-test>

  <boolean-test name="expandAllEnabled" default="true"/>
  
  <js-test/>

</test>
