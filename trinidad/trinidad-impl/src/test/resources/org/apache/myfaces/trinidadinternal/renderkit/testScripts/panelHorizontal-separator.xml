<test xmlns="http://myfaces.apache.org/adf/view/faces/test"
       xmlns:af="http://myfaces.apache.org/adf/view/faces">
  <base-component af:type="org.apache.myfaces.trinidad.CorePanelHorizontal">
    <component af:type="org.apache.myfaces.trinidad.CoreOutputText" value="1"/>
    <component af:type="org.apache.myfaces.trinidad.CoreOutputText" value="2"/>
    <component af:type="org.apache.myfaces.trinidad.CoreOutputText" value="3"/>
    <component af:type="org.apache.myfaces.trinidad.CoreObjectImage"
                af:facet="separator"
            source="/foo/bar.gif"/>
  </base-component>
  <js-test/>
  <attribute-test name="styleClass"/>
  <attribute-test name="shortDesc"/>
  <attribute-test name="inlineStyle" value="background-color:red"/>
  <enum-test name="valign"/>
  <attribute-test name="halign" value="right"/>
  <attribute-test name="halign" value="start" matchesBase="true"/>
  <attribute-test name="halign" value="left" matchesBase="true"/>
  <attribute-test name="halign" value="end"/>
  <attribute-test name="halign" value="center"/>

</test>
